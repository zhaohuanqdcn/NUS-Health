<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="orbroid.nushealth.nushealthapp">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.WIFI_STATE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/nus_health_logo_square"
        android:roundIcon="@mipmap/nus_health_logo_round"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppCompatTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TempDecl" />
        <activity android:name=".Login" />
        <activity android:name=".BookingHome" />
        <activity android:name=".BookingHistory" />
        <activity android:name=".BookingDetail" />
        <activity android:name=".BookingNewTAD" />
        <activity android:name=".BookingNewTime" />
        <activity android:name=".BookingNewDoctor" />
        <activity android:name=".BookingNewConfirm" />
        <activity android:name=".SignUp" />

    </application>

</manifest>